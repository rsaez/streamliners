<%= form_for(@health_record) do |f| %>
  
  <%= render 'shared/error_messages' %>
  <table class="table table-bordered">
  <thead class="directory">
    <tr>
      <td></td>
    </tr>
  </thead>

  <tbody>
    <%= render 'hearing' %>
    <%= render 'vision' %>
  </tbody>
  </table>
  <% if @health_record.child_id.nil?%>
    <%= f.hidden_field :child_id, value: params[:child_id]%>
  <% else %>
    <%= f.hidden_field :child_id, value: @health_record.child_id%>
  <% end %>
  <div  class: "btn btn-large btn-primary">
    <%= f.submit %>
  </div>
<% end %>
<br />
<%= link_to 'Back', @child %> |
